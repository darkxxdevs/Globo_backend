{
     "compilerOptions": {
          "outDir": "dist",
          "sourceMap": true,
          "target": "esnext",
          "module": "esnext",
          "esModuleInterop": true,
          "moduleResolution": "node",
          "forceConsistentCasingInFileNames": true,
          "noImplicitAny": true,
          "strict": true,
          "skipLibCheck": true
     },
     "include": ["./*.js", "src/**/*.ts"]
}
